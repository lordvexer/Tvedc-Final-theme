                    <div class="Slider">
                        <div id="carouselExampleCaptions" class="carousel" data-bs-ride="carousel">
                            <div class="carousel-indicators">
                                @{
                                    for (var i = 0; i < Model.News.Count; i++)
                                    {
                                        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="@i" class="@(i==0 ? "active": "")" aria-current="true" aria-label="Slide @(i+1)"></button>

                                    }
                                }
                            </div>
                            <div class="carousel-inner ">
                                @{
                                    for (var i = 0; i < Model.News.Count; i++)
                                    {
                                        var n = @Model.News[i];
                                        <div class="carousel-item @(i==0 ? "active": "")">
                                            <img src="@n.MediaHref" class="d-block w-100 Sliderimg" alt="@n.Title">
                                            <a href="@n.Href" target="_blank">
                                                <div class="carousel-caption ">
                                                    <div class="SliderTextBG">
                                                        <h5>@n.Title</h5>
                                                        <p>n.Excerpt</p>
                                                    </div>
                                                </div>
                                            </a>
                                        </div>
                                    }
                                }
                            </div>
                            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
